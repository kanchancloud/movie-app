,cn,dn=o(375205),un=o(828996),pn=o(394898),mn=o(101497),gn=o(768913),hn=o(241761),fn=o(597360);var vn,bn,xn,Cn,Dn=o(503170),kn=o(823168),yn=["onClose","value","open"],Zn=["None","Atria","Callisto","Dione","Ganymede","Hangouts Call","Luna","Oberon","Phobos","Pyxis","Sedna","Titania","Triton","Umbriel"];function Tn(n){var e=n.onClose,o=n.value,a=n.open,t=(0,E.Z)(n,yn),i=s.useState(o),l=(0,d.Z)(i,2),r=l[0],c=l[1],p=s.useRef(null);s.useEffect((function(){a||c(o)}),[o,a]);return(0,y.jsxs)(b.Z,(0,j.Z)((0,j.Z)({sx:{"& .MuiDialog-paper":{width:"80%",maxHeight:435}},maxWidth:"xs",TransitionProps:{onEntering:function(){null!=p.current&&p.current.focus()}},open:a},t),{},{children:[vn||(vn=(0,y.jsx)(v.Z,{children:"Phone Ringtone"})),(0,y.jsx)(R.Z,{dividers:!0,children:(0,y.jsx)(Dn.Z,{ref:p,"aria-label":"ringtone",name:"ringtone",value:r,onChange:function(n){c(n.target.value)},children:Zn.map((function(n){return(0,y.jsx)(pn.Z,{value:n,control:bn||(bn=(0,y.jsx)(kn.Z,{})),label:n},n)}))})}),(0,y.jsxs)(B.Z,{children:[(0,y.jsx)(u.Z,{autoFocus:!0,onClick:function(){e()},children:"Cancel"}),(0,y.jsx)(u.Z,{onClick:function(){e(r)},children:"Ok"})]})]}))}var Sn,wn=o(709497),jn=o(549529),Bn=o.n(jn);function Rn(n){return(0,y.jsx)(Bn(),{handle:"#draggable-dialog-title",cancel:'[class*="MuiDialogContent-root"]',children:(0,y.jsx)(wn.Z,(0,j.Z)({},n))})}var An,In,Pn=o(561546),Ln=o(146010);var Mn,On=o(823315);var Fn,zn={en:{description:"Dialogs inform users about a task and can contain critical information, require decisions, or involve multiple tasks.",location:"/docs/data/material/components/dialogs/dialogs.md",rendered:['<svg style="display: none;" xmlns="http://www.w3.org/2000/svg">\n  <symbol id="anchor-link-icon" viewBox="0 0 16 16">\n    <path d="M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z" />\n  </symbol>\n</svg>','<h1>Dialog</h1><p class="description">Dialogs inform users about a task and can contain critical information, require decisions, or involve multiple tasks.</p>\n\n<p>A Dialog is a type of <a href="/material-ui/react-modal/">modal</a> window that appears in front of app content to provide critical information or ask for a decision. Dialogs disable all app functionality when they appear, and remain on screen until confirmed, dismissed, or a required action has been taken.</p>\n<p>Dialogs are purposefully interruptive, so they should be used sparingly.</p>\n',{component:"modules/components/ComponentLinkHeader.js"},'<h2 id="heading-basic-dialog"><span class="anchor-link" id="basic-dialog"></span>Basic dialog<a aria-labelledby="heading-basic-dialog" class="anchor-link-style" href="#basic-dialog" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Simple dialogs can provide additional details or actions about a list item.\nFor example, they can display avatars, icons, clarifying subtext, or orthogonal actions (such as adding an account).</p>\n<p>Touch mechanics:</p>\n<ul>\n<li>Choosing an option immediately commits the option and closes the menu</li>\n<li>Touching outside of the dialog, or pressing Back, cancels the action and closes the dialog</li>\n</ul>\n',{demo:"SimpleDialog.js"},'<h2 id="heading-alerts"><span class="anchor-link" id="alerts"></span>Alerts<a aria-labelledby="heading-alerts" class="anchor-link-style" href="#alerts" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Alerts are urgent interruptions, requiring acknowledgement, that inform the user about a situation.</p>\n<p>Most alerts don&#39;t need titles.\nThey summarize a decision in a sentence or two by either:</p>\n<ul>\n<li>Asking a question (e.g. &quot;Delete this conversation?&quot;)</li>\n<li>Making a statement related to the action buttons</li>\n</ul>\n<p>Use title bar alerts only for high-risk situations, such as the potential loss of connectivity.\nUsers should be able to understand the choices based on the title and button text alone.</p>\n<p>If a title is required:</p>\n<ul>\n<li>Use a clear question or statement with an explanation in the content area, such as &quot;Erase USB storage?&quot;.</li>\n<li>Avoid apologies, ambiguity, or questions, such as &quot;Warning!&quot; or &quot;Are you sure?&quot;</li>\n</ul>\n',{demo:"AlertDialog.js"},'<h2 id="heading-transitions"><span class="anchor-link" id="transitions"></span>Transitions<a aria-labelledby="heading-transitions" class="anchor-link-style" href="#transitions" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>You can also swap out the transition, the next example uses <code>Slide</code>.</p>\n',{demo:"AlertDialogSlide.js"},'<h2 id="heading-form-dialogs"><span class="anchor-link" id="form-dialogs"></span>Form dialogs<a aria-labelledby="heading-form-dialogs" class="anchor-link-style" href="#form-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Form dialogs allow users to fill out form fields within a dialog.\nFor example, if your site prompts for potential subscribers to fill in their email address, they can fill out the email field and touch &#39;Submit&#39;.</p>\n',{demo:"FormDialog.js"},'<h2 id="heading-customization"><span class="anchor-link" id="customization"></span>Customization<a aria-labelledby="heading-customization" class="anchor-link-style" href="#customization" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Here is an example of customizing the component.\nYou can learn more about this in the <a href="/material-ui/customization/how-to-customize/">overrides documentation page</a>.</p>\n<p>The dialog has a close button added to aid usability.</p>\n',{demo:"CustomizedDialogs.js"},'<h2 id="heading-full-screen-dialogs"><span class="anchor-link" id="full-screen-dialogs"></span>Full-screen dialogs<a aria-labelledby="heading-full-screen-dialogs" class="anchor-link-style" href="#full-screen-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2>',{demo:"FullScreenDialog.js"},'<h2 id="heading-optional-sizes"><span class="anchor-link" id="optional-sizes"></span>Optional sizes<a aria-labelledby="heading-optional-sizes" class="anchor-link-style" href="#optional-sizes" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>You can set a dialog maximum width by using the <code>maxWidth</code> enumerable in combination with the <code>fullWidth</code> boolean.\nWhen the <code>fullWidth</code> prop is true, the dialog will adapt based on the <code>maxWidth</code> value.</p>\n',{demo:"MaxWidthDialog.js"},'<h2 id="heading-responsive-full-screen"><span class="anchor-link" id="responsive-full-screen"></span>Responsive full-screen<a aria-labelledby="heading-responsive-full-screen" class="anchor-link-style" href="#responsive-full-screen" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>You may make a dialog responsively full screen using <a href="/material-ui/react-use-media-query/#usemediaquery"><code>useMediaQuery</code></a>.</p>\n<pre><code class="language-jsx"><span class="token keyword">import</span> useMediaQuery <span class="token keyword">from</span> <span class="token string">\'@mui/material/useMediaQuery\'</span><span class="token punctuation">;</span>\n\n<span class="token keyword">function</span> <span class="token function">MyComponent</span><span class="token punctuation">(</span><span class="token punctuation">)</span> <span class="token punctuation">{</span>\n  <span class="token keyword">const</span> theme <span class="token operator">=</span> <span class="token function">useTheme</span><span class="token punctuation">(</span><span class="token punctuation">)</span><span class="token punctuation">;</span>\n  <span class="token keyword">const</span> fullScreen <span class="token operator">=</span> <span class="token function">useMediaQuery</span><span class="token punctuation">(</span>theme<span class="token punctuation">.</span>breakpoints<span class="token punctuation">.</span><span class="token function">down</span><span class="token punctuation">(</span><span class="token string">\'md\'</span><span class="token punctuation">)</span><span class="token punctuation">)</span><span class="token punctuation">;</span>\n\n  <span class="token keyword">return</span> <span class="token tag"><span class="token tag"><span class="token punctuation">&lt;</span><span class="token class-name">Dialog</span></span> <span class="token attr-name">fullScreen</span><span class="token script language-javascript"><span class="token script-punctuation punctuation">=</span><span class="token punctuation">{</span>fullScreen<span class="token punctuation">}</span></span> <span class="token punctuation">/></span></span><span class="token punctuation">;</span>\n<span class="token punctuation">}</span>\n</code></pre>\n',{demo:"ResponsiveDialog.js"},'<h2 id="heading-confirmation-dialogs"><span class="anchor-link" id="confirmation-dialogs"></span>Confirmation dialogs<a aria-labelledby="heading-confirmation-dialogs" class="anchor-link-style" href="#confirmation-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Confirmation dialogs require users to explicitly confirm their choice before an option is committed.\nFor example, users can listen to multiple ringtones but only make a final selection upon touching &quot;OK&quot;.</p>\n<p>Touching &quot;Cancel&quot; in a confirmation dialog, or pressing Back, cancels the action, discards any changes, and closes the dialog.</p>\n',{demo:"ConfirmationDialog.js"},'<h2 id="heading-draggable-dialog"><span class="anchor-link" id="draggable-dialog"></span>Draggable dialog<a aria-labelledby="heading-draggable-dialog" class="anchor-link-style" href="#draggable-dialog" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>You can create a draggable dialog by using <a href="https://github.com/react-grid-layout/react-draggable">react-draggable</a>.\nTo do so, you can pass the imported <code>Draggable</code> component as the <code>PaperComponent</code> of the <code>Dialog</code> component.\nThis will make the entire dialog draggable.</p>\n',{demo:"DraggableDialog.js"},'<h2 id="heading-scrolling-long-content"><span class="anchor-link" id="scrolling-long-content"></span>Scrolling long content<a aria-labelledby="heading-scrolling-long-content" class="anchor-link-style" href="#scrolling-long-content" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>When dialogs become too long for the user&#39;s viewport or device, they scroll.</p>\n<ul>\n<li><code>scroll=paper</code> the content of the dialog scrolls within the paper element.</li>\n<li><code>scroll=body</code> the content of the dialog scrolls within the body element.</li>\n</ul>\n<p>Try the demo below to see what we mean:</p>\n',{demo:"ScrollDialog.js"},'<h2 id="heading-performance"><span class="anchor-link" id="performance"></span>Performance<a aria-labelledby="heading-performance" class="anchor-link-style" href="#performance" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Follow the <a href="/material-ui/react-modal/#performance">Modal performance section</a>.</p>\n<h2 id="heading-limitations"><span class="anchor-link" id="limitations"></span>Limitations<a aria-labelledby="heading-limitations" class="anchor-link-style" href="#limitations" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Follow the <a href="/material-ui/react-modal/#limitations">Modal limitations section</a>.</p>\n<h2 id="heading-accessibility"><span class="anchor-link" id="accessibility"></span>Accessibility<a aria-labelledby="heading-accessibility" class="anchor-link-style" href="#accessibility" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Follow the <a href="/material-ui/react-modal/#accessibility">Modal accessibility section</a>.</p>\n','<h2 id="heading-api"><span class="anchor-link" id="api"></span>API<a aria-labelledby="heading-api" class="anchor-link-style" href="#api" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><ul>\n<li><a href="/material-ui/api/dialog/"><code>&lt;Dialog /&gt;</code></a></li>\n<li><a href="/material-ui/api/dialog-actions/"><code>&lt;DialogActions /&gt;</code></a></li>\n<li><a href="/material-ui/api/dialog-content/"><code>&lt;DialogContent /&gt;</code></a></li>\n<li><a href="/material-ui/api/dialog-content-text/"><code>&lt;DialogContentText /&gt;</code></a></li>\n<li><a href="/material-ui/api/dialog-title/"><code>&lt;DialogTitle /&gt;</code></a></li>\n<li><a href="/material-ui/api/slide/"><code>&lt;Slide /&gt;</code></a></li>\n</ul>\n'],toc:[{text:"Basic dialog",level:2,hash:"basic-dialog",children:[]},{text:"Alerts",level:2,hash:"alerts",children:[]},{text:"Transitions",level:2,hash:"transitions",children:[]},{text:"Form dialogs",level:2,hash:"form-dialogs",children:[]},{text:"Customization",level:2,hash:"customization",children:[]},{text:"Full-screen dialogs",level:2,hash:"full-screen-dialogs",children:[]},{text:"Optional sizes",level:2,hash:"optional-sizes",children:[]},{text:"Responsive full-screen",level:2,hash:"responsive-full-screen",children:[]},{text:"Confirmation dialogs",level:2,hash:"confirmation-dialogs",children:[]},{text:"Draggable dialog",level:2,hash:"draggable-dialog",children:[]},{text:"Scrolling long content",level:2,hash:"scrolling-long-content",children:[]},{text:"Performance",level:2,hash:"performance",children:[]},{text:"Limitations",level:2,hash:"limitations",children:[]},{text:"Accessibility",level:2,hash:"accessibility",children:[]},{text:"API",level:2,hash:"api",children:[]}],title:"React Dialog component",headers:{product:"material-ui",title:"React Dialog component",components:["Dialog","DialogActions","DialogContent","DialogContentText","DialogTitle","Slide"],githubLabel:"component: dialog",materialDesign:"https://material.io/components/dialogs",waiAria:"https://www.w3.org/TR/wai-aria-practices/#dialog_modal"}},pt:{description:"Di\xe1logos informam aos usu\xe1rios sobre uma tarefa e podem conter informa\xe7\xf5es cr\xedticas, exigir decis\xf5es ou envolver v\xe1rias tarefas.",location:"/docs/data/material/components/dialogs/dialogs-pt.md",rendered:['<svg style="display: none;" xmlns="http://www.w3.org/2000/svg">\n  <symbol id="anchor-link-icon" viewBox="0 0 16 16">\n    <path d="M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z" />\n  </symbol>\n</svg>','<h1>Di\xe1logo</h1><p class="description">Di\xe1logos informam aos usu\xe1rios sobre uma tarefa e podem conter informa\xe7\xf5es cr\xedticas, exigir decis\xf5es ou envolver v\xe1rias tarefas.</p>\n\n<p>Um Di\xe1logo \xe9 um tipo de janela <a href="/pt/material-ui/react-modal/">modal</a> que aparece na frente do conte\xfado do aplicativo para fornecer informa\xe7\xf5es cr\xedticas ou solicitar uma decis\xe3o. As caixas de di\xe1logo desativam toda a funcionalidade do aplicativo quando elas s\xe3o exibidas e permanecem na tela at\xe9 que sejam confirmadas, rejeitadas ou que uma a\xe7\xe3o necess\xe1ria tenha sido executada.</p>\n<p>Os di\xe1logos s\xe3o intencionalmente interruptivos, ent\xe3o eles devem ser usados com modera\xe7\xe3o.</p>\n',{component:"modules/components/ComponentLinkHeader.js"},'<h2 id="heading-basic-dialog"><span class="anchor-link" id="basic-dialog"></span>Di\xe1logo simples<a aria-labelledby="heading-basic-dialog" class="anchor-link-style" href="#basic-dialog" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Di\xe1logos simples podem fornecer detalhes adicionais ou a\xe7\xf5es sobre um item da lista. Por exemplo, eles podem exibir avatares, \xedcones, esclarecer subtexto ou a\xe7\xf5es ortogonais (como adicionar uma conta).</p>\n<p>Mecanismo de toque:</p>\n<ul>\n<li>Escolhendo uma op\xe7\xe3o ir\xe1 imediatamente confirmar a op\xe7\xe3o e fechar o menu</li>\n<li>Tocando para sair do di\xe1logo ou pressionando a op\xe7\xe3o de voltar ir\xe1 cancelar a a\xe7\xe3o e fechar o di\xe1logo</li>\n</ul>\n',{demo:"SimpleDialog.js"},'<h2 id="heading-alerts"><span class="anchor-link" id="alerts"></span>Alertas<a aria-labelledby="heading-alerts" class="anchor-link-style" href="#alerts" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Alertas s\xe3o interrup\xe7\xf5es urgentes, exigindo confirma\xe7\xe3o de que o usu\xe1rio esteja ciente da situa\xe7\xe3o.</p>\n<p>A maioria dos alertas n\xe3o precisam de t\xedtulos. Eles resumem uma decis\xe3o em uma senten\xe7a ou duas:</p>\n<ul>\n<li>Fazendo uma pergunta (Exemplo: &quot;Apagar esta conversa?&quot;)</li>\n<li>Fazendo uma declara\xe7\xe3o relacionada aos bot\xf5es de a\xe7\xe3o</li>\n</ul>\n<p>Use alertas com barra de t\xedtulo apenas para situa\xe7\xf5es de alto risco, como a potencial perda de conectividade. Os usu\xe1rios devem entender as escolhas com base apenas no t\xedtulo e no texto do bot\xe3o.</p>\n<p>Se um t\xedtulo \xe9 necess\xe1rio:</p>\n<ul>\n<li>Use uma pergunta ou declara\xe7\xe3o clara com uma explica\xe7\xe3o na \xe1rea de conte\xfado, como &quot;Apagar dados do USB?&quot;.</li>\n<li>Evite desculpas, ambiguidade ou senten\xe7as como &quot;Aviso!&quot; ou &quot;Voc\xea tem certeza?&quot;</li>\n</ul>\n',{demo:"AlertDialog.js"},'<h2 id="heading-transitions"><span class="anchor-link" id="transitions"></span>Transi\xe7\xf5es<a aria-labelledby="heading-transitions" class="anchor-link-style" href="#transitions" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Voc\xea tamb\xe9m pode trocar a transi\xe7\xe3o, o pr\xf3ximo exemplo utiliza <code>Slide</code>.</p>\n',{demo:"AlertDialogSlide.js"},'<h2 id="heading-form-dialogs"><span class="anchor-link" id="form-dialogs"></span>Formul\xe1rio de di\xe1logo<a aria-labelledby="heading-form-dialogs" class="anchor-link-style" href="#form-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Os formul\xe1rios de di\xe1logo permitem que usu\xe1rios preencham campos dentro de um di\xe1logo. Por exemplo, se seu site solicita aos inscritos que preencham um endere\xe7o de e-mail, eles poder\xe3o preencher o campo de e-mail e tocar em &quot;Enviar&quot;.</p>\n',{demo:"FormDialog.js"},'<h2 id="heading-customization"><span class="anchor-link" id="customization"></span>Di\xe1logos customizados<a aria-labelledby="heading-customization" class="anchor-link-style" href="#customization" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Aqui est\xe1 um exemplo de customiza\xe7\xe3o do componente. Voc\xea pode aprender mais sobre isso na <a href="/pt/material-ui/customization/how-to-customize/">p\xe1gina de documenta\xe7\xe3o de sobrescritas</a>.</p>\n<p>O di\xe1logo tem um bot\xe3o de fechar para facilitar a utiliza\xe7\xe3o.</p>\n',{demo:"CustomizedDialogs.js"},'<h2 id="heading-full-screen-dialogs"><span class="anchor-link" id="full-screen-dialogs"></span>Di\xe1logos em tela cheia<a aria-labelledby="heading-full-screen-dialogs" class="anchor-link-style" href="#full-screen-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2>',{demo:"FullScreenDialog.js"},'<h2 id="heading-optional-sizes"><span class="anchor-link" id="optional-sizes"></span>Tamanhos opcionais<a aria-labelledby="heading-optional-sizes" class="anchor-link-style" href="#optional-sizes" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Voc\xea pode definir uma largura m\xe1xima usando um enumerador na propriedade <code>maxWidth</code> combinando com a propriedade booleana <code>fullWidth</code>. Quando a propriedade <code>fullWidth</code> for verdadeira, a caixa de di\xe1logo ser\xe1 adaptada com base no valor definido em <code>maxWidth</code>.</p>\n',{demo:"MaxWidthDialog.js"},'<h2 id="heading-responsive-full-screen"><span class="anchor-link" id="responsive-full-screen"></span>Tela cheia responsiva<a aria-labelledby="heading-responsive-full-screen" class="anchor-link-style" href="#responsive-full-screen" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Voc\xea pode fazer um di\xe1logo totalmente responsivo usando <a href="/pt/material-ui/react-use-media-query/#usemediaquery"><code>useMediaQuery</code></a>.</p>\n<pre><code class="language-jsx"><span class="token keyword">import</span> useMediaQuery <span class="token keyword">from</span> <span class="token string">\'@material-ui/core/useMediaQuery\'</span><span class="token punctuation">;</span>\n\n<span class="token keyword">function</span> <span class="token function">MyComponent</span><span class="token punctuation">(</span><span class="token punctuation">)</span> <span class="token punctuation">{</span>\n  <span class="token keyword">const</span> theme <span class="token operator">=</span> <span class="token function">useTheme</span><span class="token punctuation">(</span><span class="token punctuation">)</span><span class="token punctuation">;</span>\n  <span class="token keyword">const</span> fullScreen <span class="token operator">=</span> <span class="token function">useMediaQuery</span><span class="token punctuation">(</span>theme<span class="token punctuation">.</span>breakpoints<span class="token punctuation">.</span><span class="token function">down</span><span class="token punctuation">(</span><span class="token string">\'sm\'</span><span class="token punctuation">)</span><span class="token punctuation">)</span><span class="token punctuation">;</span>\n\n  <span class="token keyword">return</span> <span class="token tag"><span class="token tag"><span class="token punctuation">&lt;</span><span class="token class-name">Dialog</span></span> <span class="token attr-name">fullScreen</span><span class="token script language-javascript"><span class="token script-punctuation punctuation">=</span><span class="token punctuation">{</span>fullScreen<span class="token punctuation">}</span></span> <span class="token punctuation">/></span></span><span class="token punctuation">;</span>\n<span class="token punctuation">}</span>\n</code></pre>\n',{demo:"ResponsiveDialog.js"},'<h2 id="heading-confirmation-dialogs"><span class="anchor-link" id="confirmation-dialogs"></span>Di\xe1logos de confirma\xe7\xe3o<a aria-labelledby="heading-confirmation-dialogs" class="anchor-link-style" href="#confirmation-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Os di\xe1logos de confirma\xe7\xe3o exigem que os usu\xe1rios confirmem explicitamente suas escolhas antes que uma op\xe7\xe3o seja confirmada. Por exemplo, os usu\xe1rios podem ouvir v\xe1rios toques, mas apenas fazer uma sele\xe7\xe3o final ao tocar em &quot;OK&quot;.</p>\n<p>Tocar em &quot;Cancelar&quot; em um di\xe1logo de confirma\xe7\xe3o ou pressionar Voltar, cancela a a\xe7\xe3o, descarta todas as altera\xe7\xf5es e fecha o di\xe1logo.</p>\n',{demo:"ConfirmationDialog.js"},'<h2 id="heading-draggable-dialog"><span class="anchor-link" id="draggable-dialog"></span>Di\xe1logo arrast\xe1vel<a aria-labelledby="heading-draggable-dialog" class="anchor-link-style" href="#draggable-dialog" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>You can create a draggable dialog by using <a href="https://github.com/react-grid-layout/react-draggable">react-draggable</a>. Para fazer isso, voc\xea deve passar o componente importado <code>Draggable</code> como <code>PaperComponent</code> do componente <code>Dialog</code>. Isso far\xe1 com que todo o di\xe1logo seja arrast\xe1vel.</p>\n',{demo:"DraggableDialog.js"},'<h2 id="heading-scrolling-long-content"><span class="anchor-link" id="scrolling-long-content"></span>Rolagem de conte\xfado longo<a aria-labelledby="heading-scrolling-long-content" class="anchor-link-style" href="#scrolling-long-content" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Quando di\xe1logos ficam muito grandes para a tela do usu\xe1rio ou dispositivo, a rolagem \xe9 habilitada.</p>\n<ul>\n<li><code>scroll=paper</code> o conte\xfado do di\xe1logo rola dentro do elemento.</li>\n<li><code>scroll=body</code> o conte\xfado do di\xe1logo rola dentro do corpo da p\xe1gina.</li>\n</ul>\n<p>Experimente a demonstra\xe7\xe3o abaixo para ver o que queremos dizer:</p>\n',{demo:"ScrollDialog.js"},'<h2 id="heading-performance"><span class="anchor-link" id="performance"></span>Performance<a aria-labelledby="heading-performance" class="anchor-link-style" href="#performance" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Veja a se\xe7\xe3o <a href="/pt/material-ui/react-modal/#performance">Performance Modal</a>.</p>\n<h2 id="heading-limitations"><span class="anchor-link" id="limitations"></span>Limita\xe7\xf5es<a aria-labelledby="heading-limitations" class="anchor-link-style" href="#limitations" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Siga a <a href="/pt/material-ui/react-modal/#limitations">se\xe7\xe3o de limita\xe7\xf5es do Modal</a>.</p>\n<h2 id="heading-accessibility"><span class="anchor-link" id="accessibility"></span>Acessibilidade<a aria-labelledby="heading-accessibility" class="anchor-link-style" href="#accessibility" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>Siga a <a href="/pt/material-ui/react-modal/#accessibility">se\xe7\xe3o de acessibilidade do Modal</a>.</p>\n','<h2 id="heading-api"><span class="anchor-link" id="api"></span>API<a aria-labelledby="heading-api" class="anchor-link-style" href="#api" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><ul>\n<li><a href="/pt/material-ui/api/dialog/"><code>&lt;Dialog /&gt;</code></a></li>\n<li><a href="/pt/material-ui/api/dialog-actions/"><code>&lt;DialogActions /&gt;</code></a></li>\n<li><a href="/pt/material-ui/api/dialog-content/"><code>&lt;DialogContent /&gt;</code></a></li>\n<li><a href="/pt/material-ui/api/dialog-content-text/"><code>&lt;DialogContentText /&gt;</code></a></li>\n<li><a href="/pt/material-ui/api/dialog-title/"><code>&lt;DialogTitle /&gt;</code></a></li>\n<li><a href="/pt/material-ui/api/slide/"><code>&lt;Slide /&gt;</code></a></li>\n</ul>\n'],toc:[{text:"Di\xe1logo simples",level:2,hash:"basic-dialog",children:[]},{text:"Alertas",level:2,hash:"alerts",children:[]},{text:"Transi\xe7\xf5es",level:2,hash:"transitions",children:[]},{text:"Formul\xe1rio de di\xe1logo",level:2,hash:"form-dialogs",children:[]},{text:"Di\xe1logos customizados",level:2,hash:"customization",children:[]},{text:"Di\xe1logos em tela cheia",level:2,hash:"full-screen-dialogs",children:[]},{text:"Tamanhos opcionais",level:2,hash:"optional-sizes",children:[]},{text:"Tela cheia responsiva",level:2,hash:"responsive-full-screen",children:[]},{text:"Di\xe1logos de confirma\xe7\xe3o",level:2,hash:"confirmation-dialogs",children:[]},{text:"Di\xe1logo arrast\xe1vel",level:2,hash:"draggable-dialog",children:[]},{text:"Rolagem de conte\xfado longo",level:2,hash:"scrolling-long-content",children:[]},{text:"Performance",level:2,hash:"performance",children:[]},{text:"Limita\xe7\xf5es",level:2,hash:"limitations",children:[]},{text:"Acessibilidade",level:2,hash:"accessibility",children:[]},{text:"API",level:2,hash:"api",children:[]}],title:"Componente Di\xe1logo para React",headers:{product:"material-ui",title:"Componente Di\xe1logo para React",components:["Dialog","DialogActions","DialogContent","DialogContentText","DialogTitle","Slide"],githubLabel:"component: dialog",materialDesign:"https://material.io/components/dialogs",waiAria:"https://www.w3.org/TR/wai-aria-practices/#dialog_modal"}},zh:{description:"\u5bf9\u8bdd\u6846\u5c06\u4e00\u4e2a\u4efb\u52a1\u544a\u77e5\u7ed9\u7528\u6237\uff0c\u5b83\u627f\u8f7d\u4e86\u4e00\u4e9b\u9700\u8981\u7528\u6237\u8fdb\u884c\u786e\u8ba4\u7684\u5173\u952e\u4fe1\u606f\u6216\u8005\u591a\u4e2a\u4efb\u52a1\u3002",location:"/docs/data/material/components/dialogs/dialogs-zh.md",rendered:['<svg style="display: none;" xmlns="http://www.w3.org/2000/svg">\n  <symbol id="anchor-link-icon" viewBox="0 0 16 16">\n    <path d="M4 9h1v1H4c-1.5 0-3-1.69-3-3.5S2.55 3 4 3h4c1.45 0 3 1.69 3 3.5 0 1.41-.91 2.72-2 3.25V8.59c.58-.45 1-1.27 1-2.09C10 5.22 8.98 4 8 4H4c-.98 0-2 1.22-2 2.5S3 9 4 9zm9-3h-1v1h1c1 0 2 1.22 2 2.5S13.98 12 13 12H9c-.98 0-2-1.22-2-2.5 0-.83.42-1.64 1-2.09V6.25c-1.09.53-2 1.84-2 3.25C6 11.31 7.55 13 9 13h4c1.45 0 3-1.69 3-3.5S14.5 6 13 6z" />\n  </symbol>\n</svg>','<h1>Dialog \u5bf9\u8bdd\u6846</h1><p class="description">\u5bf9\u8bdd\u6846\u5c06\u4e00\u4e2a\u4efb\u52a1\u544a\u77e5\u7ed9\u7528\u6237\uff0c\u5b83\u627f\u8f7d\u4e86\u4e00\u4e9b\u9700\u8981\u7528\u6237\u8fdb\u884c\u786e\u8ba4\u7684\u5173\u952e\u4fe1\u606f\u6216\u8005\u591a\u4e2a\u4efb\u52a1\u3002</p>\n\n<p>\u5bf9\u8bdd\u6846\u662f <a href="/zh/material-ui/react-modal/">modal</a> \u7a97\u4f53\u7684\u4e00\u79cd\u7c7b\u578b\uff0c\u5b83\u901a\u5e38\u5728\u5e94\u7528\u7a0b\u5e8f\u5185\u5bb9\u4e4b\u524d\u5448\u73b0\uff0c\u6765\u63d0\u4f9b\u4e00\u4e9b\u5173\u952e\u4fe1\u606f\uff0c\u6216\u8005\u8981\u6c42\u7528\u6237\u505a\u51fa\u51b3\u7b56\u3002 \u5bf9\u8bdd\u6846\u51fa\u73b0\u7684\u65f6\u5019\u4f1a\u7981\u7528\u5e94\u7528\u7a0b\u5e8f\u7684\u6240\u6709\u529f\u80fd\uff0c\u53ea\u6709\u88ab\u786e\u8ba4\u3001\u88ab\u53d6\u6d88\u6216\u5df2\u91c7\u53d6\u5176\u4ed6\u5fc5\u8981\u7684\u64cd\u4f5c\u65f6\uff0c\u5bf9\u8bdd\u6846\u4f1a\u4ece\u5c4f\u5e55\u4e2d\u6d88\u5931\u3002</p>\n<p>\u5bf9\u8bdd\u6846\u4f1a\u5e26\u6709\u76ee\u7684\u6027\u5730\u6253\u65ad\u5de5\u4f5c\u6d41\u7a0b\uff0c\u6240\u4ee5\u8bf7\u60a8\u8c28\u614e\u4f7f\u7528\u3002</p>\n',{component:"modules/components/ComponentLinkHeader.js"},'<h2 id="heading-basic-dialog"><span class="anchor-link" id="basic-dialog"></span>\u7b80\u5355\u7684\u5bf9\u8bdd\u6846<a aria-labelledby="heading-basic-dialog" class="anchor-link-style" href="#basic-dialog" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u7b80\u5355\u5bf9\u8bdd\u6846\u53ef\u4ee5\u63d0\u4f9b\u6709\u5173\u5217\u8868\u7684\u989d\u5916\u4fe1\u606f\u4e0e\u64cd\u4f5c\u3002 \u4f8b\u5982\uff0c\u5b83\u4eec\u53ef\u4ee5\u663e\u793a\u5934\u50cf\uff0c\u56fe\u6807\uff0c\u7eaf\u6587\u672c\u6216\u5177\u4f53\u52a8\u4f5c(\u4f8b\u5982\u6dfb\u52a0\u5e10\u6237)\u3002</p>\n<p>\u89e6\u6478\u64cd\u4f5c\u673a\u5236\uff1a</p>\n<ul>\n<li>\u9009\u4e2d\u4e00\u4e2a\u9009\u9879\u5219\u4f1a\u7acb\u523b\u89e6\u53d1\u63d0\u4ea4\u9009\u9879\uff0c\u5e76\u5173\u95ed\u83dc\u5355\u7684\u64cd\u4f5c</li>\n<li>\u5728\u5bf9\u8bdd\u6846\u5916\u70b9\u51fb\uff0c\u6216\u6309\u4e0b\u201c\u8fd4\u56de\u201d\uff0c\u5c06\u4f1a\u53d6\u6d88\u64cd\u4f5c\u5e76\u5173\u95ed\u5bf9\u8bdd\u6846\u3002</li>\n</ul>\n',{demo:"SimpleDialog.js"},'<h2 id="heading-alerts"><span class="anchor-link" id="alerts"></span>\u8b66\u544a\u6846<a aria-labelledby="heading-alerts" class="anchor-link-style" href="#alerts" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u8b66\u544a\u6846\u662f\u4e00\u79cd\u7d27\u6025\u4e2d\u65ad\u7684\u884c\u4e3a\uff0c\u7528\u4ee5\u901a\u77e5\u7528\u6237\uff0c\u5e76\u9700\u8981\u786e\u8ba4\u3002</p>\n<p>\u5927\u591a\u6570\u8b66\u62a5\u4e0d\u9700\u8981\u6807\u9898\u3002 \u5220\u7e41\u5c31\u7b80\uff0c\u603b\u800c\u8a00\u4e4b\uff1a</p>\n<ul>\n<li>\u95ee\u4e00\u4e2a\u95ee\u9898\uff08\u4f8b\u5982\uff1a\u201c\u662f\u5426\u5220\u9664\u6b64\u5bf9\u8bdd\uff1f\u201d\uff09</li>\n<li>\u9648\u8ff0\u4e00\u4e2a\u548c\u52a8\u4f5c\u6309\u94ae\u76f8\u5173\u7684\u58f0\u660e</li>\n</ul>\n<p>\u8bf7\u4ec5\u5728\u9ad8\u98ce\u9669\u60c5\u51b5\u4e0b\u4f7f\u7528\u6807\u9898\u680f\u8b66\u62a5\uff0c\u8003\u8651\u5230\u53ef\u80fd\u4e22\u5931\u8fde\u63a5\u3002 \u7528\u6237\u5e94\u8be5\u80fd\u591f\u5355\u51ed\u6807\u9898\u548c\u6309\u94ae\u6587\u672c\u6765\u7406\u89e3\u6240\u6709\u7684\u9009\u9879\u3002</p>\n<p>\u5982\u679c\u9700\u8981\u52a0\u4e0a\u6807\u9898\u8bf7:</p>\n<ul>\n<li>\u4f7f\u7528\u660e\u786e\u7684\u95ee\u9898\u6216\u58f0\u660e\uff0c\u5e76\u5728\u5185\u5bb9\u533a\u57df\u5bf9\u5176\u505a\u51fa\u89e3\u91ca\uff0c\u4f8b\u5982\uff1a\u201c\u662f\u5426\u8981\u6e05\u9664 USB \u4e0a\u7684\u5185\u5bb9\uff1f\u201d\u3002</li>\n<li>\u907f\u514d\u4f7f\u7528\u9053\u6b49\u3001\u6a21\u68f1\u4e24\u53ef\u7684\u5185\u5bb9\u6216\u8005\u95ee\u9898\uff0c\u4f8b\u5982\u201d\u8b66\u544a\uff01 \u201c\u6216\u8005\u201d\u4f60\u786e\u5b9a\u5417\uff1f \u201c</li>\n</ul>\n',{demo:"AlertDialog.js"},'<h2 id="heading-transitions"><span class="anchor-link" id="transitions"></span>\u8fc7\u6e21\u52a8\u753b<a aria-labelledby="heading-transitions" class="anchor-link-style" href="#transitions" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u5f53\u7136\u4f60\u4e5f\u53ef\u4ee5\u6362\u6389\u8fc7\u6e21\u6548\u679c\uff0c\u4e0b\u9762\u7684\u793a\u4f8b\u4f7f\u7528\u4e86 <code>Slide\uff08\u5e7b\u706f\u7247\uff09</code>\u3002</p>\n',{demo:"AlertDialogSlide.js"},'<h2 id="heading-form-dialogs"><span class="anchor-link" id="form-dialogs"></span>\u8868\u5355\u5bf9\u8bdd\u6846<a aria-labelledby="heading-form-dialogs" class="anchor-link-style" href="#form-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u8868\u5355\u5bf9\u8bdd\u6846\u5141\u8bb8\u7528\u6237\u5728\u5bf9\u8bdd\u6846\u5185\u586b\u5199\u8868\u5355\u3002 \u6bd4\u5982\u8bf4\uff0c\u5982\u679c\u60a8\u7684\u7f51\u7ad9\u63d0\u793a\u6f5c\u5728\u8ba2\u9605\u8005\u586b\u5199\u4ed6\u4eec\u7684\u7535\u5b50\u90ae\u4ef6\u5730\u5740\uff0c\u4ed6\u4eec\u53ef\u4ee5\u586b\u5199\u7535\u5b50\u90ae\u4ef6\u5b57\u6bb5\u7136\u540e\u70b9\u51fb\u201c\u63d0\u4ea4\u201d\u3002</p>\n',{demo:"FormDialog.js"},'<h2 id="heading-customization"><span class="anchor-link" id="customization"></span>\u81ea\u5b9a\u4e49\u5bf9\u8bdd\u6846<a aria-labelledby="heading-customization" class="anchor-link-style" href="#customization" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u4ee5\u4e0b\u662f\u81ea\u5b9a\u4e49\u7ec4\u4ef6\u7684\u4e00\u4e2a\u793a\u4f8b\u3002 \u60a8\u53ef\u4ee5\u5728 <a href="/zh/material-ui/customization/how-to-customize/">\u91cd\u5199\u6587\u6863\u9875\u9762</a> \u4e2d\u4e86\u89e3\u66f4\u591a\u6709\u5173\u6b64\u5185\u5bb9\u7684\u4fe1\u606f\u3002</p>\n<p>\u8be5\u5bf9\u8bdd\u6846\u52a0\u4e0a\u4e86\u4e00\u4e2a\u5173\u95ed\u6309\u94ae\u6765\u8f85\u52a9\u53ef\u7528\u6027\u3002</p>\n',{demo:"CustomizedDialogs.js"},'<h2 id="heading-full-screen-dialogs"><span class="anchor-link" id="full-screen-dialogs"></span>\u5168\u5c4f\u5bf9\u8bdd\u6846<a aria-labelledby="heading-full-screen-dialogs" class="anchor-link-style" href="#full-screen-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2>',{demo:"FullScreenDialog.js"},'<h2 id="heading-optional-sizes"><span class="anchor-link" id="optional-sizes"></span>\u5927\u5c0f\u9009\u62e9\u9879<a aria-labelledby="heading-optional-sizes" class="anchor-link-style" href="#optional-sizes" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u60a8\u53ef\u4ee5\u4f7f\u7528 <code>maxWidth</code>\u7684 enumerable \u548c <code>fullWidth</code>\u7684 boolean \u6765\u8bbe\u5b9a\u5bf9\u8bdd\u6846\u7684\u6700\u5927\u5bbd\u5ea6\u3002 \u5f53 <code>fullWidth</code> \u5c5e\u6027\u4e3a true \u65f6\uff0c\u5bf9\u8bdd\u6846\u5c06\u6839\u636e <code>maxWidth</code> \u7684\u503c\u8fdb\u884c\u81ea\u6211\u8c03\u6574\u3002</p>\n',{demo:"MaxWidthDialog.js"},'<h2 id="heading-responsive-full-screen"><span class="anchor-link" id="responsive-full-screen"></span>\u54cd\u5e94\u5f0f\u5168\u5c4f<a aria-labelledby="heading-responsive-full-screen" class="anchor-link-style" href="#responsive-full-screen" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u60a8\u53ef\u4ee5\u4f7f\u7528<a href="/zh/material-ui/react-use-media-query/#usemediaquery"><code>useMediaQuery</code></a>\u6765\u5b9e\u73b0\u4e00\u4e2a\u5168\u5c4f\u663e\u793a\u7684\u5bf9\u8bdd\u6846\u3002</p>\n<pre><code class="language-jsx"><span class="token keyword">import</span> useMediaQuery <span class="token keyword">from</span> <span class="token string">\'@mui/material/useMediaQuery\'</span><span class="token punctuation">;</span>\n\n<span class="token keyword">function</span> <span class="token function">MyComponent</span><span class="token punctuation">(</span><span class="token punctuation">)</span> <span class="token punctuation">{</span>\n  <span class="token keyword">const</span> theme <span class="token operator">=</span> <span class="token function">useTheme</span><span class="token punctuation">(</span><span class="token punctuation">)</span><span class="token punctuation">;</span>\n  <span class="token keyword">const</span> fullScreen <span class="token operator">=</span> <span class="token function">useMediaQuery</span><span class="token punctuation">(</span>theme<span class="token punctuation">.</span>breakpoints<span class="token punctuation">.</span><span class="token function">down</span><span class="token punctuation">(</span><span class="token string">\'md\'</span><span class="token punctuation">)</span><span class="token punctuation">)</span><span class="token punctuation">;</span>\n\n  <span class="token keyword">return</span> <span class="token tag"><span class="token tag"><span class="token punctuation">&lt;</span><span class="token class-name">Dialog</span></span> <span class="token attr-name">fullScreen</span><span class="token script language-javascript"><span class="token script-punctuation punctuation">=</span><span class="token punctuation">{</span>fullScreen<span class="token punctuation">}</span></span> <span class="token punctuation">/></span></span><span class="token punctuation">;</span>\n<span class="token punctuation">}</span>\n</code></pre>\n',{demo:"ResponsiveDialog.js"},'<h2 id="heading-confirmation-dialogs"><span class="anchor-link" id="confirmation-dialogs"></span>\u786e\u8ba4\u5bf9\u8bdd\u6846<a aria-labelledby="heading-confirmation-dialogs" class="anchor-link-style" href="#confirmation-dialogs" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u786e\u8ba4\u578b\u5bf9\u8bdd\u6846\u660e\u786e\u8981\u6c42\u7528\u6237\u5728\u63d0\u4ea4\u9009\u9879\u4e4b\u524d\u786e\u8ba4\u4ed6\u4eec\u7684\u9009\u62e9\u3002 \u4e3e\u4e2a\u4f8b\u5b50\uff0c\u7528\u6237\u53ef\u4ee5\u542c\u5230\u591a\u79cd\u94c3\u58f0\uff0c\u4f46\u662f\u53ea\u6709\u5728\u70b9\u51fb \u201cOK\u201d \u6309\u94ae\u540e\u624d\u610f\u5473\u7740\u5b8c\u6210\u4e86\u9009\u62e9\u3002</p>\n<p>\u5728\u786e\u8ba4\u5bf9\u8bdd\u6846\u4e2d\u70b9\u6309\u201c\u53d6\u6d88\u201d\u6216\u201c\u8fd4\u56de\uff08Back\uff09\u201d\u952e\uff0c\u53ef\u53d6\u6d88\u64cd\u4f5c\uff0c\u653e\u5f03\u4efb\u4f55\u66f4\u6539\uff0c\u5e76\u5173\u95ed\u5bf9\u8bdd\u6846\u3002</p>\n',{demo:"ConfirmationDialog.js"},'<h2 id="heading-draggable-dialog"><span class="anchor-link" id="draggable-dialog"></span>\u53ef\u62d6\u52a8\u7684\u5bf9\u8bdd\u6846<a aria-labelledby="heading-draggable-dialog" class="anchor-link-style" href="#draggable-dialog" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>You can create a draggable dialog by using <a href="https://github.com/react-grid-layout/react-draggable">react-draggable</a>. \u4e3a\u6b64\uff0c\u60a8\u53ef\u4ee5\u5c06\u9700\u8981\u5bfc\u5165\u7684 <code>Draggable</code> \u7ec4\u4ef6\u4f5c\u4e3a <code>Dialog</code> \u7ec4\u4ef6\u7684 <code>PaperComponent</code> \u6765\u4f20\u5165\u3002 To do so, you can pass the imported <code>Draggable</code> component as the <code>PaperComponent</code> of the <code>Dialog</code> component. \u8fd9\u6837\u4e00\u6765\uff0c\u60a8\u5c31\u53ef\u4ee5\u62d6\u52a8\u6574\u4e2a\u5bf9\u8bdd\u6846\u3002 \u8fd9\u6837\u4e00\u6765\uff0c\u60a8\u5c31\u53ef\u4ee5\u62d6\u52a8\u6574\u4e2a\u5bf9\u8bdd\u6846\u3002</p>\n',{demo:"DraggableDialog.js"},'<h2 id="heading-scrolling-long-content"><span class="anchor-link" id="scrolling-long-content"></span>\u957f\u5185\u5bb9\u6eda\u52a8<a aria-labelledby="heading-scrolling-long-content" class="anchor-link-style" href="#scrolling-long-content" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u5f53\u5bf9\u8bdd\u6846\u76f8\u5bf9\u4e8e\u7528\u6237\u7684\u89c6\u53e3\u6216\u8bbe\u5907\u6765\u8bf4\u592a\u957f\u65f6\uff0c\u5b83\u4eec\u5c31\u53ef\u4ee5\u6eda\u52a8\u3002</p>\n<ul>\n<li>\u4f7f\u7528 <code>scroll=paper</code>\uff0c\u5bf9\u8bdd\u6846\u7684\u5185\u5bb9\u80fd\u5728 paper \u5143\u7d20\u4e2d\u6eda\u52a8\u3002</li>\n<li>\u4f7f\u7528 <code>scroll=body</code>\uff0c\u5bf9\u8bdd\u6846\u7684\u5185\u5bb9\u80fd\u5728 body \u5143\u7d20\u4e2d\u6eda\u52a8\u3002</li>\n</ul>\n<p>\u8bf7\u770b\u4e00\u4e0b\u4e0b\u9762\u7684\u4f8b\u5b50\uff0c\u8fd9\u4f1a\u5e2e\u52a9\u60a8\u52a0\u6df1\u7406\u89e3\uff1a</p>\n',{demo:"ScrollDialog.js"},'<h2 id="heading-performance"><span class="anchor-link" id="performance"></span>\u6027\u80fd<a aria-labelledby="heading-performance" class="anchor-link-style" href="#performance" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u53c2\u8003<a href="/zh/material-ui/react-modal/#performance">\u5bf9\u8bdd\u6846\u6027\u80fd\u90e8\u5206</a>\u3002</p>\n<h2 id="heading-limitations"><span class="anchor-link" id="limitations"></span>\u8bbe\u8ba1\u5c40\u9650<a aria-labelledby="heading-limitations" class="anchor-link-style" href="#limitations" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u53c2\u8003<a href="/zh/material-ui/react-modal/#limitations">\u5bf9\u8bdd\u6846\u6027\u80fd\u90e8\u5206</a>\u3002</p>\n<h2 id="heading-accessibility"><span class="anchor-link" id="accessibility"></span>\u65e0\u969c\u788d\u8bbe\u8ba1<a aria-labelledby="heading-accessibility" class="anchor-link-style" href="#accessibility" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><p>\u53c2\u8003<a href="/zh/material-ui/react-modal/#accessibility">\u6a21\u6001\u6846\u65e0\u969c\u788d\u8bbe\u8ba1\u90e8\u5206</a>\u3002</p>\n','<h2 id="heading-api"><span class="anchor-link" id="api"></span>API<a aria-labelledby="heading-api" class="anchor-link-style" href="#api" tabindex="-1"><svg><use xlink:href="#anchor-link-icon" /></svg></a></h2><ul>\n<li><a href="/zh/material-ui/api/dialog/"><code>&lt;Dialog /&gt;</code></a></li>\n<li><a href="/zh/material-ui/api/dialog-actions/"><code>&lt;DialogActions /&gt;</code></a></li>\n<li><a href="/zh/material-ui/api/dialog-content/"><code>&lt;DialogContent /&gt;</code></a></li>\n<li><a href="/zh/material-ui/api/dialog-content-text/"><code>&lt;DialogContentText /&gt;</code></a></li>\n<li><a href="/zh/material-ui/api/dialog-title/"><code>&lt;DialogTitle /&gt;</code></a></li>\n<li><a href="/zh/material-ui/api/slide/"><code>&lt;Slide /&gt;</code></a></li>\n</ul>\n'],toc:[{text:"\u7b80\u5355\u7684\u5bf9\u8bdd\u6846",level:2,hash:"basic-dialog",children:[]},{text:"\u8b66\u544a\u6846",level:2,hash:"alerts",children:[]},{text:"\u8fc7\u6e21\u52a8\u753b",level:2,hash:"transitions",children:[]},{text:"\u8868\u5355\u5bf9\u8bdd\u6846",level:2,hash:"form-dialogs",children:[]},{text:"\u81ea\u5b9a\u4e49\u5bf9\u8bdd\u6846",level:2,hash:"customization",children:[]},{text:"\u5168\u5c4f\u5bf9\u8bdd\u6846",level:2,hash:"full-screen-dialogs",children:[]},{text:"\u5927\u5c0f\u9009\u62e9\u9879",level:2,hash:"optional-sizes",children:[]},{text:"\u54cd\u5e94\u5f0f\u5168\u5c4f",level:2,hash:"responsive-full-screen",children:[]},{text:"\u786e\u8ba4\u5bf9\u8bdd\u6846",level:2,hash:"confirmation-dialogs",children:[]},{text:"\u53ef\u62d6\u52a8\u7684\u5bf9\u8bdd\u6846",level:2,hash:"draggable-dialog",children:[]},{text:"\u957f\u5185\u5bb9\u6eda\u52a8",level:2,hash:"scrolling-long-content",children:[]},{text:"\u6027\u80fd",level:2,hash:"performance",children:[]},{text:"\u8bbe\u8ba1\u5c40\u9650",level:2,hash:"limitations",children:[]},{text:"\u65e0\u969c\u788d\u8bbe\u8ba1",level:2,hash:"accessibility",children:[]},{text:"API",level:2,hash:"api",children:[]}],title:"React Dialog\uff08\u5bf9\u8bdd\u6846\uff09\u7ec4\u4ef6",headers:{product:"material-ui",title:"React Dialog\uff08\u5bf9\u8bdd\u6846\uff09\u7ec4\u4ef6",components:["Dialog","DialogActions","DialogContent","DialogContentText","DialogTitle","Slide"],githubLabel:"component: dialog",materialDesign:"https://material.io/components/dialogs",waiAria:"https://www.w3.org/TR/wai-aria-practices/#dialog_modal"}}},Wn={"SimpleDialog.js":{module:"./SimpleDialog.js",raw:"import * as React from 'react';\nimport PropTypes from 'prop-types';\nimport Button from '@mui/material/Button';\nimport Avatar from '@mui/material/Avatar';\nimport List from '@mui/material/List';\nimport ListItem from '@mui/material/ListItem';\nimport ListItemAvatar from '@mui/material/ListItemAvatar';\nimport ListItemText from '@mui/material/ListItemText';\nimport DialogTitle from '@mui/material/DialogTitle';\nimport Dialog from '@mui/material/Dialog';\nimport PersonIcon from '@mui/icons-material/Person';\nimport AddIcon from '@mui/icons-material/Add';\nimport Typography from '@mui/material/Typography';\nimport { blue } from '@mui/material/colors';\n\nconst emails = ['username@gmail.com', 'user02@gmail.com'];\n\nfunction SimpleDialog(props) {\n  const { onClose, selectedValue, open } = props;\n\n  const handleClose = () => {\n    onClose(selectedValue);\n  };\n\n  const handleListItemClick = (value) => {\n    onClose(value);\n  };\n\n  return (\n    <Dialog onClose={handleClose} open={open}>\n      <DialogTitle>Set backup account</DialogTitle>\n      <List sx={{ pt: 0 }}>\n        {emails.map((email) => (\n          <ListItem button onClick={() => handleListItemClick(email)} key={email}>\n            <ListItemAvatar>\n              <Avatar sx={{ bgcolor: blue[100], color: blue[600] }}>\n                <PersonIcon />\n              </Avatar>\n            </ListItemAvatar>\n            <ListItemText primary={email} />\n          </ListItem>\n        ))}\n\n        <ListItem autoFocus button onClick={() => handleListItemClick('addAccount')}>\n          <ListItemAvatar>\n            <Avatar>\n              <AddIcon />\n            </Avatar>\n          </ListItemAvatar>\n          <ListItemText primary=\"Add account\" />\n        </ListItem>\n      </List>\n    </Dialog>\n  );\n}\n\nSimpleDialog.propTypes = {\n  onClose: PropTypes.func.isRequired,\n  open: PropTypes.bool.isRequired,\n  selectedValue: PropTypes.string.isRequired,\n};\n\nexport default function SimpleDialogDemo() {\n  const [open, setOpen] = React.useState(false);\n  const [selectedValue, setSelectedValue] = React.useState(emails[1]);\n\n  const handleClickOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = (value) => {\n    setOpen(false);\n    setSelectedValue(value);\n  };\n\n  return (\n    <div>\n      <Typography variant=\"subtitle1\" component=\"div\">\n        Selected: {selectedValue}\n      </Typography>\n      <br />\n      <Button variant=\"outlined\" onClick={handleClickOpen}>\n        Open simple dialog\n      </Button>\n      <SimpleDialog\n        selectedValue={selectedValue}\n        open={open}\n        onClose={handleClose}\n      />\n    </div>\n  );\n}\n",jsxPreview:'<Typography variant="subtitle1" component="div">\n  Selected: {selectedValue}\n</Typography>\n<br />\n<Button variant="outlined" onClick={handleClickOpen}>\n  Open simple dialog\n</Button>\n<SimpleDialog\n  selectedValue={selectedValue}\n  open={open}\n  onClose={handleClose}\n/>',moduleTS:"./SimpleDialog.js",rawTS:"import * as React from 'react';\nimport Button from '@mui/material/Button';\nimport Avatar from '@mui/material/Avatar';\nimport List from '@mui/material/List';\nimport ListItem from '@mui/material/ListItem';\nimport ListItemAvatar from '@mui/material/ListItemAvatar';\nimport ListItemText from '@mui/material/ListItemText';\nimport DialogTitle from '@mui/material/DialogTitle';\nimport Dialog from '@mui/material/Dialog';\nimport PersonIcon from '@mui/icons-material/Person';\nimport AddIcon from '@mui/icons-material/Add';\nimport Typography from '@mui/material/Typography';\nimport { blue } from '@mui/material/colors';\n\nconst emails = ['username@gmail.com', 'user02@gmail.com'];\n\nexport interface SimpleDialogProps {\n  open: boolean;\n  selectedValue: string;\n  onClose: (value: string) => void;\n}\n\nfunction SimpleDialog(props: SimpleDialogProps) {\n  const { onClose, selectedValue, open } = props;\n\n  const handleClose = () => {\n    onClose(selectedValue);\n  };\n\n  const handleListItemClick = (value: string) => {\n    onClose(value);\n  };\n\n  return (\n    <Dialog onClose={handleClose} open={open}>\n      <DialogTitle>Set backup account</DialogTitle>\n      <List sx={{ pt: 0 }}>\n        {emails.map((email) => (\n          <ListItem button onClick={() => handleListItemClick(email)} key={email}>\n            <ListItemAvatar>\n              <Avatar sx={{ bgcolor: blue[100], color: blue[600] }}>\n                <PersonIcon />\n              </Avatar>\n            </ListItemAvatar>\n            <ListItemText primary={email} />\n          </ListItem>\n        ))}\n        <ListItem autoFocus button onClick={() => handleListItemClick('addAccount')}>\n          <ListItemAvatar>\n            <Avatar>\n              <AddIcon />\n            </Avatar>\n          </ListItemAvatar>\n          <ListItemText primary=\"Add account\" />\n        </ListItem>\n      </List>\n    </Dialog>\n  );\n}\n\nexport default function SimpleDialogDemo() {\n  const [open, setOpen] = React.useState(false);\n  const [selectedValue, setSelectedValue] = React.useState(emails[1]);\n\n  const handleClickOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = (value: string) => {\n    setOpen(false);\n    setSelectedValue(value);\n  };\n\n  return (\n    <div>\n      <Typography variant=\"subtitle1\" component=\"div\">\n        Selected: {selectedValue}\n      </Typography>\n      <br />\n      <Button variant=\"outlined\" onClick={handleClickOpen}>\n        Open simple dialog\n      </Button>\n      <SimpleDialog\n        selectedValue={selectedValue}\n        open={open}\n        onClose={handleClose}\n      />\n    </div>\n  );\n}\n"},"AlertDialogSlide.js":{module:"./AlertDialogSlide.js",raw:"import * as React from 'react';\nimport Button from '@mui/material/Button';\nimport Dialog from '@mui/material/Dialog';\nimport DialogActions from '@mui/material/DialogActions';\nimport DialogContent from '@mui/material/DialogContent';\nimport DialogContentText from '@mui/material/DialogContentText';\nimport DialogTitle from '@mui/material/DialogTitle';\nimport Slide from '@mui/material/Slide';\n\nconst Transition = React.forwardRef(function Transition(props, ref) {\n  return <Slide direction=\"up\" ref={ref} {...props} />;\n});\n\nexport default function AlertDialogSlide() {\n  const [open, setOpen] = React.useState(false);\n\n  const handleClickOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  return (\n    <div>\n      <Button variant=\"outlined\" onClick={handleClickOpen}>\n        Slide in alert dialog\n      </Button>\n      <Dialog\n        open={open}\n        TransitionComponent={Transition}\n        keepMounted\n        onClose={handleClose}\n        aria-describedby=\"alert-dialog-slide-description\"\n      >\n        <DialogTitle>{\"Use Google's location service?\"}</DialogTitle>\n        <DialogContent>\n          <DialogContentText id=\"alert-dialog-slide-description\">\n            Let Google help apps determine location. This means sending anonymous\n            location data to Google, even when no apps are running.\n          </DialogContentText>\n        </DialogContent>\n        <DialogActions>\n          <Button onClick={handleClose}>Disagree</Button>\n          <Button onClick={handleClose}>Agree</Button>\n        </DialogActions>\n      </Dialog>\n    </div>\n  );\n}\n",moduleTS:"./AlertDialogSlide.js",rawTS:"import * as React from 'react';\nimport Button from '@mui/material/Button';\nimport Dialog from '@mui/material/Dialog';\nimport DialogActions from '@mui/material/DialogActions';\nimport DialogContent from '@mui/material/DialogContent';\nimport DialogContentText from '@mui/material/DialogContentText';\nimport DialogTitle from '@mui/material/DialogTitle';\nimport Slide from '@mui/material/Slide';\nimport { TransitionProps } from '@mui/material/transitions';\n\nconst Transition = React.forwardRef(function Transition(\n  props: TransitionProps & {\n    children: React.ReactElement<any, any>;\n  },\n  ref: React.Ref<unknown>,\n) {\n  return <Slide direction=\"up\" ref={ref} {...props} />;\n});\n\nexport default function AlertDialogSlide() {\n  const [open, setOpen] = React.useState(false);\n\n  const handleClickOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  return (\n    <div>\n      <Button variant=\"outlined\" onClick={handleClickOpen}>\n        Slide in alert dialog\n      </Button>\n      <Dialog\n        open={open}\n        TransitionComponent={Transition}\n        keepMounted\n        onClose={handleClose}\n        aria-describedby=\"alert-dialog-slide-description\"\n      >\n        <DialogTitle>{\"Use Google's location service?\"}</DialogTitle>\n        <DialogContent>\n          <DialogContentText id=\"alert-dialog-slide-description\">\n            Let Google help apps determine location. This means sending anonymous\n            location data to Google, even when no apps are running.\n          </DialogContentText>\n        </DialogContent>\n        <DialogActions>\n          <Button onClick={handleClose}>Disagree</Button>\n          <Button onClick={handleClose}>Agree</Button>\n        </DialogActions>\n      </Dialog>\n    </div>\n  );\n}\n"},"FormDialog.js":{module:"./FormDialog.js",raw:"import * as React from 'react';\nimport Button from '@mui/material/Button';\nimport TextField from '@mui/material/TextField';\nimport Dialog from '@mui/material/Dialog';\nimport DialogActions from '@mui/material/DialogActions';\nimport DialogContent from '@mui/material/DialogContent';\nimport DialogContentText from '@mui/material/DialogContentText';\nimport DialogTitle from '@mui/material/DialogTitle';\n\nexport default function FormDialog() {\n  const [open, setOpen] = React.useState(false);\n\n  const handleClickOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  return (\n    <div>\n      <Button variant=\"outlined\" onClick={handleClickOpen}>\n        Open form dialog\n      </Button>\n      <Dialog open={open} onClose={handleClose}>\n        <DialogTitle>Subscribe</DialogTitle>\n        <DialogContent>\n          <DialogContentText>\n            To subscribe to this website, please enter your email address here. We\n            will send updates occasionally.\n          </DialogContentText>\n          <TextField\n            autoFocus\n            margin=\"dense\"\n            id=\"name\"\n            label=\"Email Address\"\n            type=\"email\"\n            fullWidth\n            variant=\"standard\"\n          />\n        </DialogContent>\n        <DialogActions>\n          <Button onClick={handleClose}>Cancel</Button>\n          <Button onClick={handleClose}>Subscribe</Button>\n        </DialogActions>\n      </Dialog>\n    </div>\n  );\n}\n",moduleTS:"./FormDialog.js",rawTS:"import * as React from 'react';\nimport Button from '@mui/material/Button';\nimport TextField from '@mui/material/TextField';\nimport Dialog from '@mui/material/Dialog';\nimport DialogActions from '@mui/material/DialogActions';\nimport DialogContent from '@mui/material/DialogContent';\nimport DialogContentText from '@mui/material/DialogContentText';\nimport DialogTitle from '@mui/material/DialogTitle';\n\nexport default function FormDialog() {\n  const [open, setOpen] = React.useState(false);\n\n  const handleClickOpen = () => {\n    setOpen(true);\n  };\n\n  const handleClose = () => {\n    setOpen(false);\n  };\n\n  return (\n    <div>\n      <Button variant=\"outlined\" onClick={handleClickOpen}>\n        Open form dialog\n      </Button>\n      <Dialog open={open} onClose={handleClose}>\n        <DialogTitle>Subscribe</DialogTitle>\n        <DialogContent>\n          <DialogContentText>\n            To subscribe to this website, please enter your email address here. We\n            will send updates occasionally.\n          </DialogContentText>\n          <TextField\n            autoFocus\n            margin=\"dense\"\n            id=\"name\"\n            label=\"Email Address\"\n            type=\"email\"\n            fullWidth\n            variant=\"standard\"\n          />\n        </DialogContent>\n        <DialogActions>\n          <Button onClick={handleClose}>Cancel</Button>\n          <Button onClick={handleClose}>Subscribe</Button>\n        </DialogActions>\n      </Dialog>\n    </div>\n  );\n}\n"},"CustomizedDialogs.js":{module:"./CustomizedDialogs.js",raw:"import * as React from 'react';\nimport PropTypes from 'prop-types';\nimport Button from '@mui/material/Button';\nimport { styled } from '@mui/material/styles';\nimport Dialog from '@mui/material/Dialog';\nimport DialogTitle from '@mui/material/DialogTitle';\nimport DialogContent from '@mui/material/DialogContent';\nimport DialogActions from '@mui/material/DialogActions';\nimport IconButton from '@mui/material/IconButton';\nimport CloseIcon from '@mui/icons-material/Close';\nimport Typography from '@mui/material/Typography';\n\nconst BootstrapDialog = styled(Dialog)(({ theme }) => ({\n  '& .MuiDialogContent-root': {\n    padding: theme.spacing(2),\n  },\n  '& .MuiDialo